[program:chownweb2py]
command = chown -R www-data:www-data /home/www-data/web2py

[program:app-uwsgi]
command = /usr/local/bin/uwsgi --ini /etc/uwsgi/web2py.ini

[program:nginx-app]
command = /usr/sbin/nginx

[program:nginx-app]
command = python  /home/www-data/web2py/web2py.py --nogui -i 0.0.0.0 -p 8888 -a "<recycle>"
